view {
  font-size: 14px;
  padding-bottom: 8px;
  font-family: Arial, "PingFang SC", "Hiragino Sans GB", STHeiti, "Microsoft YaHei", "WenQuanYi Micro Hei", sans-serif;
}

.detail {
  width: 100%;
}

.hidden {
  width: 70%;
  padding-right: 22%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.title_coloect .detail_title {
  font-size: 16px;
}
.title_coloect {
  font-weight: bold;
  box-sizing: border-box;
  padding: 1% 2%;
  margin-top: 10px;
}
.coloect{
  display: inline-block;
  float: right;
  margin-left: 14rpx;
}

.coloect image {
  width: 20px;
  height: 20px;
}

.content {
  box-sizing: border-box;
  padding: 0 2%;
}